<h1>New Receipt</h1>


<%= form_for @receipt do |f| %>
  <br>

  <%= f.text_field :notes %>
  <br>
  <%= f.file_field :document_image %>
  <br>
   <%= f.collection_select(:transaction_id, @transactions, :id, :description) %>
  <br>
  <div class="actions">
  <br>
  <%= submit_tag 'Submit', class: 'btn btn-success' %>
  </div>
<% end %>

<%= link_to 'Back', receipts_path %>
